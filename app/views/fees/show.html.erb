<%- model_class = Fee -%>
<div class="page-header">
  <h1>Detalles<%#=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:Usuario) %>:</strong></dt>
  <dd><%= @fee.user_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Cantidad) %>:</strong></dt>
  <dd><%= @fee.cantidadDinero %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Tiempo) %>:</strong></dt>
  <dd><%= @fee.tiempo %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("Ver registros")),
              fees_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("Editar")),
              edit_fee_path(@fee), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("Eliminar")),
              fee_path(@fee),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("¿Está seguro de eliminar este registro?", :default => '¿Está seguro de eliminar este registro?')) },
              :class => 'btn btn-danger' %></div>
